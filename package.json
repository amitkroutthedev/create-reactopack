{
  "name": "create-rp-app",
  "version": "1.0.5",
  "type": "module",
  "description": "It's a npx command tool to create a react project with the help of webpack and give choices to user to add important packages and popular css frameworks.",
  "license": "MIT",
  "exports": "./dist/index.js",
  "bin": {
    "create-rp-app": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "start": "node ./dist/index.js"
  },
  "files": [
    "packages",
    ".gitignore",
    "LICENSE",
    "package-lock.json",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "axios",
    "redux",
    "react-router",
    "redux",
    "react",
    "npx",
    "mui",
    "redux-thunk",
    "redux-saga",
    "tailwindcss",
    "bootstrap"
  ],
  "author": "amitkrout",
  "bugs": {
    "url": "https://github.com/amitkroutthedev/create-rp-app/issues"
  },
  "homepage": "https://github.com/amitkroutthedev/create-rp-app#readme",
  "dependencies": {
    "@inquirer/prompts": "4.1.0",
    "chalk": "5.3.0",
    "figlet": "1.7.0",
    "gradient-string": "^2.0.2",
    "inquirer": "9.2.15",
    "shelljs": "0.8.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/gradient-string": "^1.1.5",
    "@types/shelljs": "^0.8.15",
    "esbuild": "0.19.11",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
